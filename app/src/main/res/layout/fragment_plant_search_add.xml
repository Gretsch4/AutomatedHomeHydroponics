<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.Plant_Vitals.PlantVitalsFragment">

    <TextView
        android:id="@+id/textViewDIST2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="284dp"
        android:layout_marginBottom="16dp"
        android:text="Hours"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberHRS"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextNumberHRS"
        android:layout_width="110dp"
        android:layout_height="52dp"
        android:layout_marginBottom="20dp"
        android:autofillHints=""

        android:ems="10"
        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberHRS"


        app:layout_constraintBottom_toTopOf="@+id/button_add_plant"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.196"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberAIRTEMP2"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginStart="116dp"
        android:layout_marginBottom="9dp"
        android:autofillHints=""

        android:ems="10"
        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberAIRTEMP"
        app:layout_constraintBottom_toTopOf="@+id/textViewDIST2"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberWATERTEMP2"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginEnd="132dp"
        android:layout_marginBottom="20dp"
        android:autofillHints=""
        android:ems="10"

        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberWATERTEMP"
        app:layout_constraintBottom_toTopOf="@+id/textViewDIST"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextNumberPH2"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="8dp"
        android:autofillHints=""
        android:clickable="false"
        android:ems="10"
        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberPH"

        app:layout_constraintBottom_toTopOf="@+id/textViewLIGHT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.664"
        app:layout_constraintStart_toEndOf="@+id/editTextNumberTDS" />

    <EditText
        android:id="@+id/editTextNumberTDS2"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginStart="124dp"
        android:layout_marginBottom="12dp"
        android:autofillHints=""
        android:clickable="false"
        android:ems="10"
        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberTDS"
        app:layout_constraintBottom_toTopOf="@+id/textViewHUMID"

        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberDIST"
        android:layout_width="110dp"
        android:layout_height="52dp"
        android:layout_marginStart="232dp"
        android:layout_marginBottom="20dp"

        android:autofillHints=""
        android:ems="10"
        android:inputType="numberDecimal"


        android:labelFor="@id/editTextNumberDIST"
        app:layout_constraintBottom_toTopOf="@+id/button_add_plant"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberWATERTEMP"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginEnd="64dp"
        android:layout_marginBottom="20dp"
        android:autofillHints=""
        android:ems="10"

        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberWATERTEMP"
        app:layout_constraintBottom_toTopOf="@+id/textViewDIST"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextNumberAIRTEMP"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="9dp"
        android:autofillHints=""

        android:ems="10"
        android:inputType="numberDecimal"

        android:labelFor="@id/editTextNumberAIRTEMP"
        app:layout_constraintBottom_toTopOf="@+id/textViewDIST2"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberLIGHT"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginEnd="68dp"
        android:layout_marginBottom="10dp"
        android:autofillHints=""
        android:ems="10"

        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberLIGHT"
        app:layout_constraintBottom_toTopOf="@+id/textViewWATERTEMP"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextNumberLIGHT2"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginEnd="136dp"
        android:layout_marginBottom="8dp"
        android:autofillHints=""
        android:ems="10"

        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberLIGHT"
        app:layout_constraintBottom_toTopOf="@+id/textViewWATERTEMP"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextNumberHUMID"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="8dp"
        android:autofillHints=""
        android:ems="10"

        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberHUMID"
        app:layout_constraintBottom_toTopOf="@+id/textViewAIRTEMP"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberHUMID2"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginStart="116dp"
        android:layout_marginBottom="8dp"
        android:autofillHints=""
        android:ems="10"

        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberHUMID"
        app:layout_constraintBottom_toTopOf="@+id/textViewAIRTEMP"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberPH"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginEnd="70dp"
        android:layout_marginBottom="10dp"
        android:autofillHints=""
        android:clickable="false"
        android:ems="10"
        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberPH"

        app:layout_constraintBottom_toTopOf="@+id/textViewLIGHT"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/editTextNumberTDS" />

    <TextView
        android:id="@+id/textViewHUMID"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="70dp"
        android:layout_marginBottom="12dp"
        android:text="@string/sensor_humidity"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberHUMID"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textViewLIGHT"
        android:layout_width="41dp"
        android:layout_height="21dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="8dp"
        android:text="@string/sensor_light"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberLIGHT"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textViewPH"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:layout_marginEnd="116dp"
        android:text="@string/sensor_ph"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberPH"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.908" />

    <TextView
        android:id="@+id/textViewWATERTEMP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="8dp"
        android:text="@string/sensor_water_temp"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberWATERTEMP"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textViewDIST"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="88dp"
        android:layout_marginBottom="16dp"
        android:text="@string/sensor_distance"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberDIST"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textViewTDS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="100dp"
        android:layout_marginTop="30dp"
        android:text="@string/sensor_tds"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberTDS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.908" />

    <TextView
        android:id="@+id/textViewAIRTEMP"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginBottom="10dp"
        android:text="@string/sensor_air_temp"
        app:layout_constraintBottom_toTopOf="@+id/editTextNumberAIRTEMP"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:id="@+id/editTextNumberTDS"
        android:layout_width="50dp"
        android:layout_height="52dp"
        android:layout_marginStart="44dp"
        android:layout_marginBottom="12dp"
        android:autofillHints=""
        android:clickable="false"
        android:ems="10"
        android:inputType="numberDecimal"
        android:labelFor="@id/editTextNumberTDS"
        app:layout_constraintBottom_toTopOf="@+id/textViewHUMID"

        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button_add_plant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="56dp"
        android:text="Add Plant"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>
